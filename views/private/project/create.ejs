<section id="inner-headline">
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <ul class="breadcrumb">
        <li><a href="#"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
        <li><a href="/admin">Admin</a><i class="icon-angle-right"></i></li>
        <li><a href="/admin/project/list"><%= __('project') %></a><i class="icon-angle-right"></i></li>
        <li class="active"><%= __('create') %></li>
      </ul>
    </div>
  </div>
</div>
</section>
<section id="content">
<div class="container">

<div class="row">
  <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
      <form role="form" class="register-form" action="/project/create" method="post">
    <h2><%= __('create')%> <%= __('project') %> </h2>
    <hr class="colorgraph">
    <div class="row">
      <div class="form-group">
            <input type="text" name="Project[title]"  id="title" class="form-control input-lg" placeholder="<%= __('title') %>" tabindex="2">
      </div>
      <div class="form-group">
            <input type="text" name="Project[asesor]"  id="asesor" class="form-control input-lg" placeholder="<%= __('asesor') %>" tabindex="2">
      </div>
      <div class="form-group">

        <select class="form-control input-lg" id="program" name="Project[program]">
          <option value=""><%= __('select-pro') %></option>
            <% for(var i=0; i<programs.length; i++) { %>
                    <option value="<%= programs[i].id%>"><%= programs[i].name %></option>
            <% } %>
        </select>

      </div>
      <div class="form-group">
        <select class="form-control input-lg" id="period" name="Project[period]" required="true">
            <option value=""><% __('select-pe') %></option>
            <% for(var i=0; i<periods.length; i++) { %>
                    <option value="<%= periods[i].id%>"><%= periods[i].name %></option>
            <% } %>
        </select>
      </div>
      <div class="form-group">
        <select class="form-control input-lg" id="status" name="Project[status]" required="true">
            <option value=""><% __('select-status') %></option>
                    <option value="1"><% __('desarrollo') %></option>
                    <option value="2"><% __('terminado') %></option>
        </select>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-6">
        <div class="form-group">
            <input type="text" name="Project[author1]" id="author1" class="form-control input-lg" placeholder="<% __('autor-1') %>" tabindex="1">
        </div>
      </div>
        <div class="col-xs-12 col-sm-6 col-md-6">
          <div class="form-group">
            <input type="text" name="Project[author2]" id="author2" class="form-control input-lg" placeholder="<% __('autor-2') %>" tabindex="2">
          </div>
        </div>
        <div class="form-group">
          <input type="text" name="Project[author3]" id="author3" class="form-control input-lg" placeholder="<% __('autor-3') %>" tabindex="2">
        </div>
        <div class="form-group">
          <textarea type="text" name="Project[description]" id="description" class="form-control input-lg" placeholder="<% __('short-desc') %>" tabindex="2"></textarea>
        </div>
        <div class="form-group">
          <textarea type="text" rows="8" name="Project[descriptionAll]" id="descriptionAll" class="form-control input-lg" placeholder="<% __('all-desc') %>" tabindex="2"></textarea>
        </div>

    <div class="row">
      <div class="col-xs-8 col-sm-9 col-md-9">
         <strong class="label label-primary">Admin</strong>
      </div>
    </div>

    <hr class="colorgraph">
    <div class="row">
      <div class="col-xs-12 col-md-6"><input type="submit" value="Save" class="btn btn-theme btn-block btn-lg" tabindex="7"></div>
    </div>
  </form>
</div>
</div>
</div>
</section>
