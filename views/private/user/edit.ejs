<section id="inner-headline">
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <ul class="breadcrumb">
        <li><a href="#"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
        <li><a href="/admin">Admin</a><i class="icon-angle-right"></i></li>
        <li><a href="/admin/user/list"><% __('user') %></a><i class="icon-angle-right"></i></li>
        <li class="active"><% __('edit') %></li>
      </ul>
    </div>
  </div>
</div>
</section>
<section id="content">
<div class="container">

<div class="row">
  <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
      <form role="form" class="register-form" action="/user/edit" method="post">
    <h2><% __('edit') %> <% __('user') %> </h2>
    <hr class="colorgraph">
    <div class="row">
      <div class="form-group">
            <input type="hidden" name="User[id]"  id="name" class="form-control input-lg" placeholder="" tabindex="2" value="<%= user.id %>">
            <input type="text" name="User[name]"  id="name" class="form-control input-lg" placeholder="<% __('name') %>" tabindex="2" value="<%= user.name %>">
      </div>
      <div class="form-group">
            <input type="email" name="User[email]"  id="name" class="form-control input-lg" placeholder="<% __('email') %>" tabindex="2" value="<%= user.email %>">
      </div>
      <div class="form-group">
            <input type="text" name="User[password]"  id="name" class="form-control input-lg" placeholder="<% __('pass') %>" tabindex="2" value="<%= user.password %>">
      </div>
    <div class="row">
      <div class="col-xs-8 col-sm-9 col-md-9">
         <strong class="label label-primary">By Admin</strong>
      </div>
    </div>

    <hr class="colorgraph">
    <div class="row">
      <div class="col-xs-12 col-md-6"><input type="submit" value="<% __('save') %>" class="btn btn-theme btn-block btn-lg" tabindex="7"></div>
    </div>
  </form>
</div>
</div>
</div>
</section>
