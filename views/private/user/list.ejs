<section id="inner-headline">
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <ul class="breadcrumb">
        <li><a href="#"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
        <li><a href="/admin">Admin</a><i class="icon-angle-right"></i></li>
        <li><a href="/admin/user/list">User</a><i class="icon-angle-right"></i></li>
        <li class="active">List</li>
      </ul>
    </div>
  </div>
</div>
</section>
<section id="content">
<div class="container">

<div class="row">
  <a href="/admin/user/create" class="btn btn-primary"><i class="glyphicon glyphicon-plus
"></i> Crear</a>
  <br><br>
  <div class="col-lg-12">
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>#</th>
          <th>Name</th>
          <th>Email</th>
          <th>Password</th>
          <th>Type User</th>
          <th>Date</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i<user.length; i++) { %>
              <tr>
                  <td class="highlight">
                      <div class="success"></div>
                      <a href="/admin/user/edit?id=<%= user[i].id %>"><%= user[i].id %> </a>
                  </td>
                  <td><a href="/admin/user/edit?id=<%= user[i].id %>"><%= user[i].name %> </a></td>
                  <td><%= user[i].email %></td>
                  <td><%= user[i].password %></td>
                  <td><%= user[i].typeuser %></td>
                  <td><%= user[i].date.toISOString() %></td>
                  <td>
                    <a href="/user/delete?id=<%= user[i].id%>" class="btn btn-theme"><i class="glyphicon glyphicon-trash
                  "></i> Delete</a>
                  </td>
              <tr>
          <% } %>
      </tbody>
    </table>
    <div class="btn-toolbar">
        <div class="btn-group">
          <a href="#" class="btn btn-default">1</a>
          <a href="#" class="btn btn-default">2</a>
          <a href="#" class="btn btn-default">3</a>
          <a href="#" class="btn btn-default">4</a>
        </div>

        <div class="btn-group">
          <a href="#" class="btn btn-default">5</a>
          <a href="#" class="btn btn-default">6</a>
          <a href="#" class="btn btn-default">7</a>
        </div>
    </div>
  </div>
</div>
</div>
<!-- Modal -->
