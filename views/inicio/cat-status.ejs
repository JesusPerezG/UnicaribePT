<!-- Portfolio Projects -->
	<div class="container marginbot50">
		<div class="row">
			<div class="col-lg-12">
				<h4 class="heading">Estatus Proyecto</h4>
					<a href="/catalogo/status/search?q=1&p=1" class="btn btn-danger btn-lg">Desarrollo</a>
					<a href="/catalogo/status/search?q=2&p=1" class="btn btn-danger btn-lg">Terminados</a>


				<div id="grid-container" class="cbp-l-grid-projects col 6 md 6 ">
					<ul >
							<% for(var i=0; i<project.length; i++) { %>
								<li class="cbp-item prog-3 col 6 md 6">
									<div class="row col 6 md 6">

											<div class="col 6 md 6">
												<div class="panel panel-default">
		    									<div class="panel-heading"><h5><%=project[i].title%></h5></div>
		       										 <div class="panel-body">
		            									<ul class="list-group">
		               										 <li class="list-group-item"><h6>Autor  <span class="glyphicon glyphicon-user"</span></h6> <%=project[i].author1%> / <%=project[i].author2%> / <%=project[i].author3%> </li>
		                									 <li class="list-group-item"><h6>Asesor    <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></h6><%=project[i].asesor%> </li>
		                									<li class="list-group-item"><h6>Programa Educativo    <span class="glyphicon glyphicon-book" aria-hidden="true"></span></h6><%=project[i].program%></li>
		                									<li class="list-group-item"><td><%= project[i].date.toISOString() %></td></li>
		            									</ul>
		       										 </div>
		    									</div>
											</div>
										</div>
								</li>
								<% } %>
					</ul>
				</div>
				<nav>
				  <ul class="pager">
				    <% if(page>1) {%>
				        <li><a href="/catalogo/status/search?q=<%=pr%>&p=<%=page-1%>">Anterior</a></li>
				    <% } %>
				    <% if(count > (page*8)) {%>
				        <li><a href="/catalogo/status/search?q=<%=pr%>&p=<%=parseInt(page)+1%>">Siguiente</a></li>
				    <% } %>
				  </ul>
				</nav>


				<!--div class="cbp-l-loadMore-button">
					<a href="ajax/loadMore.html" class="cbp-l-loadMore-button-link">LOAD MORE</a>
				</div-->

			</div>
		</div>
		</div>


		<!-- divider -->
		<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<div class="solidline">
				</div>
			</div>
		</div>
		</div>
		<!-- end divider -->
