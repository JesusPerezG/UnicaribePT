<% for(var i=0; i<project.length; i++) { %>
 <div class="row ">
   <div class="col-md-6 col-md-offset-3">
       <div class="panel panel-default">
           <!-- Default panel contents -->
           <div class="panel-heading"><h4><%= project[i].title %></h4></div>
           <div class="panel-body">
             <ul class="list-group">
                   <li class="list-group-item"><h5>Autor   <span class="glyphicon glyphicon-user"</span></h5> <%= project[i].author1 %> /  <%= project[i].author2 %> / <%= project.author3 %></li>
                   <li class="list-group-item"><h5>Asesor    <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></h5> <%= project[i].asesor %> </li>
                   <li class="list-group-item"><h5>Programa Educativo    <span class="glyphicon glyphicon-book" aria-hidden="true"></span></h5><%= project[i].program %></li>
                   <li class="list-group-item"><%= project[i].date.toISOString() %> </li>
                   <li class="list-group-item"><a href="your link" class="btn btn-primary"><i class="glyphicon glyphicon-download"></i> Reporte</a></li>
             </ul>
         </div>
       </div>
   </div>
 </div>
<% } %>

