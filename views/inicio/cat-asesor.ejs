<% if(count>0) { %>
  <nav>
  <div class="row">
    <div class="col-md-4 col-md-offset-5">
        <div id="pagination">
          <ul class="pager">
        	 <span class="all">Page <%=page%> of <%=Math.round(count/5)%></span>
        	 <span class="current"><%=page%></span>
           <% if(page>1) {%>
             <a href="/catalogo/asesor/search?q=<%=letter%>&p=<%=page-1%>">Anterior</a>
           <% } %>
           <% if(count > (page*5)) {%>
        	    <a href="/catalogo/asesor/search?q=<%=letter%>&p=<%=parseInt(page)+1%>">Siguiente</a>
            <% } %>
        </div>
      </div>
    </div>
  </nav>
<% } %>
